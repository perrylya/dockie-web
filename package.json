{
  "name": "dockie",
  "productName": "dockie",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "backend-dev": "nodemon server/index.js --exec babel-node --presets es2015,stage-2",
    "backend-build": "babel server -d server-dist --presets es2015,stage-2",
    "backend-serve": "node server-dist/index.js"
  },
  "keywords": [],
  "author": "Perry Ya",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "ignore": [
          "server",
          "server-dist"
        ]
      },
      "electronWinstallerConfig": {
        "name": "dockie"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "dockie"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "draft-js": "^0.10.5",
    "draft-js-counter-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.1.1",
    "draft-js-side-toolbar-plugin": "^2.0.4",
    "draft-js-static-toolbar-plugin": "^2.0.2",
    "draft.js": "^0.2.0",
    "electron-compile": "6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "material-ui": "^0.20.1",
    "mongoose": "^5.2.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cli": "^1.0.1",
    "electron-prebuilt-compile": "2.0.4",
    "socket.io": "^2.1.1"
  }
}
